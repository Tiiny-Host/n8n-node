{
	"name": "n8n-nodes-tiiny-host",
	"version": "0.1.14",
	"description": "n8n community node to interact with Tiiny Host - Upload, update, and manage your static sites",
	"license": "MIT",
	"homepage": "https://tiiny.host",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"tiiny",
		"tiiny-host",
		"static-hosting",
		"website-hosting"
	],
	"author": {
		"name": "Tiiny Host",
		"email": "support@tiiny.host"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Tiiny-Host/n8n-node.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "pnpm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/TiinyApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Tiiny/Tiiny.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "^0.14.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}